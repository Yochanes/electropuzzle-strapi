  },
  "peerDependencies": {
    "@strapi/strapi": "^4.0.0"
  },
  "author": {
    "name": "Ricardo Gomez @rgomeztinoco"
  },
  "maintainers": [
    {
      "name": "Ricardo Gomez @rgomeztinoco",
      "email": "rgomeztinoco@protonmail.com"
    }
  ],
  "engines": {
    "node": ">=14.19.1 <=18.x.x",
    "npm": ">=6.0.0"
  },
  "license": "MIT",
  "keywords": [
    "strapi",
    "strapi plugin",
    "heroicons",
    "strapi custom field",
    "strapi icons",
    "strapi heroicons"
